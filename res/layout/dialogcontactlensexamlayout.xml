    android:id="@+id/lllensexam">

    <TextView
        android:id="@+id/tvlensexameligibility"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tvlensexamfrequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tvlensexamcopay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tvlensexamceverage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tvlensexamdesc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    
</LinearLayout>